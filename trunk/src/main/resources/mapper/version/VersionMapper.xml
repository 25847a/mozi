<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fadl.version.dao.VersionMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.fadl.version.entity.Version">
        <id column="id" property="id" />
        <result column="versionNumber" property="versionNumber" />
        <result column="versionDescription" property="versionDescription" />
        <result column="isDelete" property="isDelete" />
        <result column="createDate" property="createDate" />
        <result column="updateDate" property="updateDate" />
        <result column="creater" property="creater" />
        <result column="updater" property="updater" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, versionNumber, versionDescription, isDelete, createDate, updateDate, creater, updater
    </sql>

</mapper>
